	keywords = {aridisols, Biochemistry, general, desertification, erosion, Geochemistry, geostatistics, Larrea tridentata, nitrogen, phosphorus, Prosopis glandulosa, soil heterogeneity, Soil Science \& Conservation, Terrestrial Pollution},
	pages = {169--187},
	file = {Full Text PDF:C\:\\Users\\steve\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\d7dwu3qm.default\\zotero\\storage\\EKR9I8II\\Schlesinger and Pilmanis - 1998 - Plant-soil Interactions in Deserts.pdf:application/pdf;Snapshot:C\:\\Users\\steve\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\d7dwu3qm.default\\zotero\\storage\\7VK2VRUC\\A1005939924434.html:text/html}
