@article{sheldon_behaviour_1974,
	title = {The {Behaviour} of {Seeds} in {Soil}: {III}. {The} {Influence} of {Seed} {Morphology} and the {Behaviour} of {Seedlings} on the {Establishment} of {Plants} from {Surface}-{Lying} {Seeds}},
	volume = {62},
	copyright = {Copyright © 1974 British Ecological Society},
	issn = {0022-0477},
	shorttitle = {The {Behaviour} of {Seeds} in {Soil}},
	url = {http://www.jstor.org/stable/2258879},
	doi = {10.2307/2258879},
	abstract = {The position of a seed on a soil surface has a significant effect upon germination. The fixation position of the achene in a number of Compositae is imposed by the pappus, and ensures that the area of the achene most efficient in taking up water for germination is in contact with the substrate. Other morphological adaptations of seeds which achieve this are discussed. Modifications to the microsites on a soil surface, as imposed by obstructions which modify achene-moisture relationships, affect seed germination and establishment. The differences in response, both between species and treatments, depend on the germination requirements of the seed. The subtlety of microsite conditions can be illustrated only indirectly, because no apparatus exists, at the scale of seed size, for monitoring micro-climatic conditions. Increased soil compaction reduces germination through the reduction of available microsites, while establishment is reduced because of the mechanical barrier to radicle penetration. The anchoring effect of morphological features of the achene may aid radicle penetration by allowing it to exert maximum growth pressure. Soil crusting may reduce germination and establishment, especially when the clay content of the soil is high. The impact of raindrops changes soil microtopography and may improve conditions for germination, although it may also disturb and damage slow-growing seedlings. The different behaviour and response of seeds and seedlings of individual species, to surface soil conditions, regulate the seedling population that establishes at any one site, and may be an important element maintaining floristic diversity.},
	number = {1},
	urldate = {2015-05-02},
	journal = {Journal of Ecology},
	author = {Sheldon, J. C.},
	month = mar,
	year = {1974},
	pages = {47--66},
	file = {JSTOR Full Text PDF:C\:\\Users\\steve\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\d7dwu3qm.default\\zotero\\storage\\Z3D2VEX7\\Sheldon - 1974 - The Behaviour of Seeds in Soil III. The Influence.pdf:application/pdf}
}

@article{davidson_effects_1993,
	title = {The {Effects} of {Herbivory} and {Granivory} on {Terrestrial} {Plant} {Succession}},
	volume = {68},
	copyright = {Copyright © 1993 Nordic Society Oikos},
	issn = {0030-1299},
	url = {http://www.jstor.org/stable/3545305},
	doi = {10.2307/3545305},
	abstract = {Based on resource availability models of plant defense investment, several authors have predicted and documented that herbivores tend to accelerate the pace of plant community succession. A literature survey of experimental and observational tests of the effects of above-ground, native herbivores on terrestrial plant succession reveals a more complex pattern than anticipated from earlier reviews on this subject. Although herbivory tends to hasten succession from grasses, shrubs, and pioneer trees to persistent trees (often including conifers), it typically retards succession from earlier seres, if such seres are present (e.g., as in secondary succession of old fields). The dichotomy in herbivore effects early and late in old field succession reflects differentially high herbivory on intermediate seral species (again grasses, shrubs and pioneer trees), whose principal defense against herbivores is rapid, compensatory growth in comparatively favorable resource environments. In plant communities ranging from boreal forests to African savannas, both herbivore preferences, and their effects on community succession, may be predictable from the underlying plant resource regimes and plant defenses evolved in response to these regimes. Individually or as a "functional group", palatable species might be regarded as "keystone resources", whose fluctuating abundances are likely to influence communities of both producers and consumer species on a local and landscape scale. In contrast to herbivory, granivory is predicted and found to fall differentially on larger-seeded, later successional species, and to inhibit succession in a diversity of plant communities. In this way, granivores often behave as "keystone predators". At present, there are too few data to place confidence in the generality of this result. An understanding of patterns in the effects of consumers on plant community development may be useful for both managing natural ecosystems and restoring disturbed habitats.},
	number = {1},
	urldate = {2015-05-01},
	journal = {Oikos},
	author = {Davidson, Diane W.},
	month = oct,
	year = {1993},
	pages = {23--35},
	file = {JSTOR Full Text PDF:C\:\\Users\\steve\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\d7dwu3qm.default\\zotero\\storage\\J9BWHU36\\Davidson - 1993 - The Effects of Herbivory and Granivory on Terrestr.pdf:application/pdf}
}

